{
    // -------------------------------------------------------------------------
    // Global configuration
    // -------------------------------------------------------------------------
    "layer": "top",
    "position": "top",
    "height": 20,
    "margin-left": 6,
    "margin-bottom": 0,
    "margin-right": 6,
    "margin-top": 6,
    "spacing": 2, // Gaps between modules (4px)
    "modules-left": [
        "custom/notification",
        "sway/workspaces",
        "sway/mode",
        "sway/scratchpad",
        "custom/media",
    ],
    "modules-center": [
        "clock",
        "sway/window"
    ],
    "modules-right": [
        "tray",
        "custom/weather",
        //"backlight",
        "cpu",
        "battery",
        "memory",
        "custom/storage",
        "custom/pacman",
        //"pulseaudio",
        "wireplumber",
        //"custom/screenshot_t",
        "custom/power"
    ],
    // -------------------------------------------------------------------------
    // Modules
    // -------------------------------------------------------------------------
    "custom/sp1": {
        "format": " | ",
        "tooltip": false
    },
    "custom/sp2": {
        "format": " |",
        "tooltip": false
    },
    "custom/rofi": {
        "format": "ÔÉâ",
        "tooltip": false,
        "on-click-right": "nwg-drawer",
        "on-click": "wofi --show run",
        "on-click-middle": "pkill -9 wofi"
    },
    "custom/screenshot_t": {
        "format": "ÔÄ∞",
        "on-click": "~/.config/hypr/scripts/screenshot_full",
        "on-click-right": "~/.config/hypr/scripts/screenshot_area"
    },
    /*
    "clock#1": {
        "format": "ÔÅ≥ {:%a}",
        "tooltip": false,
        "on-click": "gsimplecal"
    },
    "clock#2": {
        "format": "Ôâ¥ {:%d-%h-%Y}",
        "tooltip": false,
        "on-click": "gsimplecal"
    },
    "clock#3": {
        "format": "ÔÄó {:%H:%M:%S %p}",
        "tooltip": false,
        "on-click": "gsimplecal"
    },
    */
    "temperature": {
        // "thermal-zone": 1,
        "interval": 4,
        //"hwmon-path": "/sys/class/hwmon/hwmon3/temp1_input",
        "critical-threshold": 80,
        // "format-critical": "Ôãâ {temperatureC}¬∞C",
        "format-critical": "ÔÅ±  {temperatureC}¬∞C",
        "format": "{icon}  {temperatureC}¬∞C",
        "format-icons": [
            "Ôù´",
            "Ôãâ",
            "Ôù©"
        ],
        "max-length": 7,
        "min-length": 7,
        "on-click": "xsensors"
    },
    "memory": {
        "interval": 30,
        "format": "Óâ¶",
        "tooltip": true,
        "tooltip-format": "Óâ¶  {used:0.2f} / {total:0.0f} GB",
        "on-click": "kitty -e btop"
    },
    "battery": {
        "interval": 2,
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 15
        },
        "format": "{icon} {capacity}%",
        "format-charging": "ÔñÉ {capacity}%",
        "format-plugged": "Ôá¶ {capacity}%",
        "format-icons": [
            "ÔâÑ",
            "ÔâÉ",
            "ÔâÇ",
            "ÔâÅ",
            "ÔâÄ"
        ]
    },
    "network": {
        "format-wifi": "Ôá´ {essid} ({signalStrength}%)",
        "format-ethernet": "{ifname}: {ipaddr}/{cidr} Ôûñ",
        "format-linked": "{ifname} (No IP) Ôûñ",
        "format": "ÔÄí",
        "format-disconnected": "Ôöî",
        "format-alt": "{ifname}: {ipaddr}/{cidr}",
        "on-click": "wl-copy $(ip address show up scope global | grep inet | head -n1 | cut -d/ -f 1 | tr -d [:space:] | cut -c5-)",
        "on-click-right": "wl-copy $(ip address show up scope global | grep inet6 | head -n1 | cut -d/ -f 1 | tr -d [:space:] | cut -c6-)",
        "tooltip-format": "ÔÇì {bandwidthUpBits} ÔÄô {bandwidthDownBits}\n{ifname}\n{ipaddr}/{cidr}\n",
        "tooltip-format-wifi": "Ôá´ {essid} {frequency}MHz\nStrength: {signaldBm}dBm ({signalStrength}%)\nIP: {ipaddr}/{cidr}\nÔÇì {bandwidthUpBits} ÔÄô {bandwidthDownBits}",
        "interval": 10
    },
    "custom/storage": {
        "format": "ÔÇ†",
        "format-alt": "{percentage}% ÔÇ†",
        "format-alt-click": "click-right",
        "return-type": "json",
        "interval": 60,
        "exec": "~/.config/waybar/modules/storage.sh"
    },
    "backlight": {
        "device": "intel_backlight",
        "format": "{icon} {percent}%",
        "format-alt": "{percent}% {icon}",
        "format-alt-click": "click-right",
        //"format-icons": ["ÔÉ´", "Ô†¥"],
        "format-icons": [
            "ÔÜÖ",
            "ÔÑë"
        ],
        "on-scroll-down": "brightnessctl s 5%-",
        "on-scroll-up": "brightnessctl s +5%"
    },
    "custom/weather": {
        "format": "{}",
        "format-alt": "{alt}:{}",
        "format-alt-click": "click-right",
        "interval": 3600,
        "exec": "curl -s 'https://wttr.in/?format=1'",
        //"return-type": "json",
        //"exec": "~/.config/waybar/modules/weather.sh",
        "exec-if": "ping wttr.in -c1"
    },
    "custom/pacman": {
        "format": "Ôìπ {}",
        "format-alt": "Ôìπ {}",
        "interval": 3600, // every hour
        "exec": "checkupdates | wc -l", // # of updates
        "exec-if": "exit 0", // always run; consider advanced run conditions
        "on-click": "termite -e 'sudo pacman -Syu'; pkill -SIGRTMIN+8 waybar", // update system
        "signal": 8
    },
    "custom/media": {
        "format": "{icon} {}",
        "return-type": "json",
        "max-length": 40,
        "format-icons": {
            "spotify": "ÔÜº",
            "default": "üéú"
        },
        "escape": true,
        "exec": "$HOME/.config/waybar/mediaplayer.py 2> /dev/null" // Script in resources folder
        // "exec": "$HOME/.config/waybar/mediaplayer.py --player spotify 2> /dev/null" // Filter player based on name
    },
    "custom/spotify": {
        "format": "{icon} {}",
        "escape": true,
        "return-type": "json",
        "max-length": 40,
        "interval": 30, // Remove this if your script is endless and write in loop
        "on-click": "playerctl -p spotify play-pause",
        "on-click-right": "killall spotify",
        "smooth-scrolling-threshold": 10, // This value was tested using a trackpad, it should be lowered if using a mouse.
        "on-scroll-up": "playerctl -p spotify next",
        "on-scroll-down": "playerctl -p spotify previous",
        "exec": "$HOME/.config/waybar/mediaplayer.py 2> /dev/null", // Script in resources/custom_modules folder
        "exec-if": "pgrep spotify"
    },
    "custom/power": {
        "format": "ÔÄë ",
        "tooltip": false,
        "on-click": "wlogout"
    },
    "clock": {
        "format": "ÔÄó  {:%H:%M}",
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "today-format": "<b color='#a6e3a1'>{}</b>"
    },
    "clock#date": {
        "format": "Û∞•î {:%H:%M | ÔÅ≥%e %b}", // Icon: calendar-alt
        //"format": "{:%I:%M %p  %A %b %d}",
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "today-format": "<b >{}</b>"
    },
    "custom/gammastep": {
        "interval": 5,
        "return-type": "json",
        "exec": {
            "pre": "if unit_status=\"$(systemctl --user is-active gammastep)\"; then\nstatus=\"$unit_status ($(journalctl --user -u gammastep.service -g 'Period: ' | tail -1 | cut -d ':' -f6 | xargs))\"\nelse\nstatus=\"$unit_status\"\nfi",
            "alt": "${status:-inactive}",
            "tooltip": "Gammastep is $status",
        },
        "format": "{icon}",
        "format-icons": {
            "activating": "Û∞Å™ ",
            "deactivating": "Û∞Å™ ",
            "inactive": "? ",
            "active (Night)": "ÔÜÜ ",
            "active (Nighttime)": "ÔÜÜ ",
            "active (Transition (Night)": "ÔÜÜ ",
            "active (Transition (Nighttime)": "ÔÜÜ ",
            "active (Day)": "Óåç ",
            "active (Daytime)": "Óåç ",
            "active (Transition (Day)": "Óåç ",
            "active (Transition (Daytime)": "Óåç ",
        },
        "on-click": "systemctl --user is-active gammastep && systemctl --user stop gammastep || systemctl --user start gammastep",
    },
    "cpu": {
        "interval": 10,
        "format": "Ôãõ",
    },
    "mpd": {
        "max-length": 25,
        "format": "<span foreground='#bb9af7'>ÔÄÅ</span> {title}",
        "format-paused": "ÔÄÅ {title}",
        "format-stopped": "<span foreground='#bb9af7'>ÔÄÅ</span>",
        "format-disconnected": "",
        "on-click": "mpc --quiet toggle",
        "on-click-right": "mpc update; mpc ls | mpc add",
        "on-click-middle": "alacritty -e ncmpcpp",
        "on-scroll-up": "mpc --quiet prev",
        "on-scroll-down": "mpc --quiet next",
        "smooth-scrolling-threshold": 5,
        "tooltip-format": "{title} - {artist} ({elapsedTime:%M:%S}/{totalTime:%H:%M:%S})"
    },
    "custom/title": {
        "format": "{}",
        "interval": 0,
        "return-type": "json",
        //"max-length": 35,
        "tooltip": false
    },
    "custom/title#name": {
        "format": "{}",
        "interval": 0,
        "return-type": "json",
        "max-length": 35,
        "exec": "$HOME/.scripts/title"
    },
    "custom/notification": {
        "tooltip": false,
        "format": "{icon}",
        "format-icons": {
            "notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
            "none": "ÔÇ¢",
            "dnd-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
            "dnd-none": "Ôá∑",
            "inhibited-notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
            "inhibited-none": "ÔÇ¢",
            "dnd-inhibited-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
            "dnd-inhibited-none": "Ôá∑"
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "swaync-client -swb",
        "on-click": "swaync-client -t -sw",
        "on-click-right": "swaync-client -d -sw",
        "escape": true
    },
